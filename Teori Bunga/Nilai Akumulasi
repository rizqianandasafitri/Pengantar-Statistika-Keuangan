#a: nilai akumulasi
  #i: bunga
  #t: tahun
  #m: per 3 bulanan
  #k :present value
  nilaiakumulasigabungan <- function(num, k,t,i,m=TRUE)
  switch(num, 
         satu = {
           bungatunggal = k*(1+i)^t
           print(bungatunggal)
         },
         dua = {
           bungamajemuknominal = k*(1+i/m)^(m*t)
           print(bungamajemuknominal)
         },
         tiga = {
           bungamajemukkontinu = k*exp(i*t)
           print(bungamajemukkontinu)
         }
  )
